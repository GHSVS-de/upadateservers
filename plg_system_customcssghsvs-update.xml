		<downloads>
			<downloadurl type="full" format="zip">https://github.com/GHSVS-de/plg_system_customcssghsvs/raw/2021.08.06/dist/plg_system_customcssghsvs-2021.08.06.zip</downloadurl>
		</downloads>
		<tags>
			<tag>stable</tag>
		</tags>
		<maintainer>G@HService Berlin Neukölln, Volkmar Volli Schlothauer</maintainer>
		<maintainerurl>https://www.ghsvs.de</maintainerurl>
		<targetplatform name="joomla" version=".*" />
		<php_minimum>7.3.0</php_minimum>
	</update>
<!--/PLG_SYSTEM_CUSTOMCSSGHSVS Version 2021.08.06-->

