	<update>
		<name>PLG_SYSTEM_BACKUPSLOGGHSVS</name>
		<description>Joomla plugin that copies db entries of Akeeba Backup and Easy Joomla Backup into a tab separated csv file inside each backups folder. Runs only on login into backend.</description>
		<element>backupslogghsvs</element>
		<type>plugin</type>
		<folder>system</folder>
		<client>site</client>
		<version>2021.08.22</version>
		<infourl>https://updates.ghsvs.de/changelog.php?file=backupslogghsvs</infourl>

		<changelogurl>https://raw.githubusercontent.com/GHSVS-de/upadateservers/master/backupslogghsvs-changelog.xml</changelogurl>

		<downloads>
			<downloadurl type="full" format="zip">https://github.com/GHSVS-de/plg_system_backupslogghsvs/archive/2021.08.22.zip</downloadurl>
		</downloads>
		<tags>
			<tag>stable</tag>
		</tags>
		<maintainer>G@HService Berlin Neukölln, Volkmar Volli Schlothauer</maintainer>
		<maintainerurl>https://www.ghsvs.de</maintainerurl>
		<targetplatform name="joomla" version=".*" />
		<!--<php_minimum>7.0.0</php_minimum>-->
	</update>

